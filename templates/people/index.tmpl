{{template "top" .}}

<div class="people">
  <div class="width">
    <h1>Lists of Estonian verbs</h1>

    <div class="people-table">
      <div class="person-row head">
        <div class="person-row-first">Estonian</div>
        <div class="person-row-last">English</div>
      </div>

      {{ range.people }}
      <div class="person-row">
        <div class="person-row-first">
          {{ .FirstName }}
        </div>
        <div class="person-row-last">
          {{ .LastName }}
        </div>
      </div>
      {{ end }}
    </div>
  </div>
</div>

{{template "bottom" .}}
